<row valign="top">
<entry valign="top" align="left"><m:math><m:mi>b</m:mi></m:math></entry>
<entry valign="top" align="left"><m:math><m:mrow><m:mfrac><m:mrow><m:msup><m:mi>b</m:mi><m:mrow><m:mn>−0.00001</m:mn></m:mrow></m:msup><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow><m:mrow><m:mn>−0.00001</m:mn></m:mrow></m:mfrac><m:mo>&lt;</m:mo><m:msup><m:mi>B</m:mi><m:mo>′</m:mo></m:msup><m:mrow><m:mo>(</m:mo><m:mn>0</m:mn><m:mo>)</m:mo></m:mrow><m:mo>&lt;</m:mo><m:mfrac><m:mrow><m:msup><m:mi>b</m:mi><m:mrow><m:mn>0.00001</m:mn></m:mrow></m:msup><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow><m:mrow><m:mn>0.00001</m:mn></m:mrow></m:mfrac></m:mrow></m:math></entry>
<entry valign="top" align="left"><m:math><m:mi>b</m:mi></m:math></entry>
