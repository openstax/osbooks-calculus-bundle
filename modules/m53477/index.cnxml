<entry align="left"><m:math><m:mrow><m:mtext mathvariant="bold-italic">f</m:mtext><m:mo stretchy="false" mathvariant="bold">(</m:mo><m:mtext mathvariant="bold-italic">x</m:mtext><m:mo stretchy="false" mathvariant="bold">)</m:mo></m:mrow></m:math></entry>
<entry align="left">0</entry>
<entry align="left">4</entry>
