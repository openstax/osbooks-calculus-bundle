<table id="fs-id1170572550754" class="column-header" summary="A table with 2 rows and 5 columns. The first row is labeled “x” and has the values “-4; -2; 0; 2; 4”. The second row is labeled “g(x)” and has the values “1; 0; 3; 0; 5”.">
<tgroup cols="6">
<colspec colnum="1" colname="c1"/>
<colspec colnum="2" colname="c2"/>
<colspec colnum="3" colname="c3"/>
<colspec colnum="4" colname="c4"/>
<colspec colnum="5" colname="c5"/>
<colspec colnum="6" colname="c6"/>
<tbody>
<row valign="top">
<entry align="left"><m:math><m:mtext mathvariant="bold-italic">x</m:mtext></m:math></entry>
<entry align="left"><m:math><m:mrow><m:mn>−4</m:mn></m:mrow></m:math></entry>
<entry align="left"><m:math><m:mrow><m:mn>−2</m:mn></m:mrow></m:math></entry>
<entry align="left">0</entry>
<entry align="left">2</entry>
<entry align="left">4</entry>
</row>
<row valign="top">
<entry align="left"><m:math><m:mrow><m:mtext mathvariant="bold-italic">g</m:mtext><m:mo stretchy="false" mathvariant="bold">(</m:mo><m:mtext mathvariant="bold-italic">x</m:mtext><m:mo stretchy="false" mathvariant="bold">)</m:mo></m:mrow></m:math></entry>
<entry align="left">1</entry>
<entry align="left">0</entry>
<entry align="left">3</entry>
<entry align="left">0</entry>
<entry align="left">5</entry>
</row>
</tbody>
</tgroup>
